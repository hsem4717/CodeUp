#include<stdio.h>
int main(){
    char c;
    int a, b=0;
    scanf("%d",&a);
    b = a;
    while(1){
        scanf("%c",&c);
        if(c == '+'){
            scanf("%d",&a);
            b += a;}
        else if(c == '-'){
            scanf("%d",&a);
            b -= a;}
        else if(c == '/'){
            scanf("%d",&a);
            b /= a;}
        else if(c == '*'){
            scanf("%d",&a);
            b *= a;}
        else if(c == '='){
            printf("%d",b);
            break;
        }
    }
    return 0;
}
