#include <stdio.h>

long long int n;



long long int sqrt(long long int N){
    long long int a = 0;
    for (long long int i = 0; i <= N; i++) {
        if (i * i > N) {
            break;
        }
        else {
            a = i;
        }
    }
    return a;
}
int main()
{
  scanf("%lld", &n);
  printf("%d\n", sqrt(n));
  return 0;
}
